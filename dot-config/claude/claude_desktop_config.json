{
  "api_key": "SET_OUTSIDE_REPO",
  "note": "Sanitized placeholder committed to repo. Provide your actual key via environment variable, OS keychain, or external secrets manager.",
  "managed_by": "dotfiles",
  "last_updated_utc": "2025-10-03T01:09:00Z",
  "isUsingBuiltInNodeForMcp": true,
  "mcpServers": {
    "[claude] Context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "tools": ["get-library-docs", "resolve-library-id"]
    },
    "[claude] fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "[claude] filesystem": {
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/s-a-c/Desktop",
        "/Users/s-a-c/Downloads",
        "/Users/s-a-c/Herd",
        "/Users/s-a-c/code",
        "/Users/s-a-c/dotfiles",
        "/Users/s-a-c/nc",
        "/Users/s-a-c/.config"
      ]
    },
    "[claude] git": {
      "command": "uvx",
      "args": [
        "mcp-server-git",
        "--repository",
        "${ZF_GIT_ROOT}"
      ]
    },
    "[claude] github": {
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GH_TOKEN}"
      }
    },
    "[claude] jetbrains": {
      "command": "bunx",
      "args": [
        "-y",
        "@jetbrains/mcp-proxy"
      ]
    },
    "[claude] json": {
      "command": "bunx",
      "args": [
        "-y",
        "@gongrzhe/server-json-mcp@latest"
      ]
    },
    "[claude] mcp-installer": {
      "command": "bunx",
      "args": [
        "-y",
        "@anaisbetts/mcp-installer"
      ]
    },
    "[claude] memory": {
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "[claude] nx-mcp": {
      "command": "bunx",
      "args": [
        "-y",
        "nx-mcp@latest"
      ]
    },
    "[claude] sequential-thinking": {
      "command": "bunx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "[claude] serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server"
      ]
    },
    "[claude] tavily-mcp": {
      "command": "bunx",
      "args": [
        "-y",
        "tavily-mcp@latest"
      ],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "[claude] time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    }
  }
}
