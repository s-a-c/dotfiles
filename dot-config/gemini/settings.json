{
  "mcpServers": {
    "Context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "command": "npx",
      "type": "stdio"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/Users/s-a-c/Desktop",
        "/Users/s-a-c/Downloads"
      ],
      "command": "npx"
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "GITHUB_PAT_REDACTED"
      }
    },
    "jetbrains": {
      "args": [
        "-y",
        "@jetbrains/mcp-proxy"
      ],
      "command": "npx"
    },
    "json": {
      "args": [
        "@gongrzhe/server-json-mcp@1.0.3"
      ],
      "command": "npx"
    },
    "mcp-installer": {
      "args": [
        "@anaisbetts/mcp-installer"
      ],
      "command": "npx"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx"
    },
    "tavily-mcp": {
      "args": [
        "-y",
        "tavily-mcp@latest"
      ],
      "command": "npx",
      "env": {
        "TAVILY_API_KEY": "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"
      }
    },
    "time": {
      "args": [
        "mcp-server-time --local-timezone Europe/London"
      ],
      "command": "uvx"
    }
  },
  "selectedAuthType": "gemini-api-key",
  "theme": "Ayu"
}