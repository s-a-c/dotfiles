Host *
    IgnoreUnknown AddKeysToAgent,UseKeychain
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_ed25519
    # Explicitly exclude FIDO keys to prevent prompts
    IdentitiesOnly yes

Host github.com
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_ed25519
    # Only use the specific key, avoid FIDO keys
    IdentitiesOnly yes
