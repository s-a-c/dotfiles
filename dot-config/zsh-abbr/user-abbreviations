abbr -g "G"="| grep"
abbr -g "H"="| head"
abbr -g "L"="| less"
abbr -g "S"="| sort"
abbr -g "T"="| tail"
abbr -g "W"="| wc -l"
abbr "..."="cd ../.."
abbr ".."="cd .."
abbr "d"="docker"
abbr "dc"="docker-compose"
abbr "dps"="docker ps"
abbr "g"="git"
abbr "ga"="git add"
abbr "gb"="git branch"
abbr "gc"="git commit"
abbr "gco"="git checkout"
abbr "gd"="git diff"
abbr "gl"="git pull"
abbr "gp"="git push"
abbr "la"="ls -la"
abbr "ll"="ls -la"
